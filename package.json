{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "node scripts/inject-sw-version.js && vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist && esbuild api/index.ts --platform=node --packages=external --bundle --format=esm --outdir=.vercel/output/functions/api",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate": "tsx server/migrate.ts",
    "db:init-vercel": "tsx scripts/init-vercel-db.ts",
    "vercel-build": "node scripts/inject-sw-version.js && vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
