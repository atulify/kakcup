    "@types/express-session": "^1.18.0",
    "@types/node": "20.16.11",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.13",
    "@vercel/node": "^3.2.29",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.30.4",
